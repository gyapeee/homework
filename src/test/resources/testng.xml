<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Homework Suite" verbose="1" parallel="methods" thread-count="6">
    <listeners>
        <listener class-name="runner.AnnotationTransformer"/>
    </listeners>
    <test name="Regression for homework">
        <packages>
            <package name="homework.*"/>
        </packages>
    </test>
</suite>